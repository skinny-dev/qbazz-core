    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prebuild": "echo 'Building TypeScript...'",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
